        <Button x:Name="LeavesButton" Content="Leaves" HorizontalAlignment="Left" Margin="42,85,0,0" VerticalAlignment="Top" Width="75" Click="LeavesButton_Click"/>
        <Button x:Name="WaterStreamButton" Content="Water Stream" HorizontalAlignment="Left" Margin="138,85,0,0" VerticalAlignment="Top" Width="75" Click="WaterStreamButton_Click"/>
