        <Button x:Name="CoffeeShopButton" Content="Coffee Shop" HorizontalAlignment="Left" Margin="42,161,0,0" VerticalAlignment="Top" Width="75" Click="CoffeeShopButton_Click"/>
        <Button x:Name="TrainButton" Content="Train" HorizontalAlignment="Left" Margin="138,161,0,0" VerticalAlignment="Top" Width="75" Click="TrainButton_Click"/>
        <Button x:Name="FanButton" Content="Fan" HorizontalAlignment="Left" Margin="42,187,0,0" VerticalAlignment="Top" Width="75" Click="FanButton_Click"/>
        <Button x:Name="WhiteNoiseButton" Content="White Noise" HorizontalAlignment="Left" Margin="138,187,0,0" VerticalAlignment="Top" Width="75" Click="WhiteNoiseButton_Click"/>
        <Button x:Name="PinkNoiseButton" Content="Pink Noise" HorizontalAlignment="Left" Margin="42,212,0,0" VerticalAlignment="Top" Width="75" Click="PinkNoiseButton_Click"/>
